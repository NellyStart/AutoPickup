-- –ü–æ–¥–∫–ª—é—á–∞–µ–º –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–µ —Å–µ—Ä–≤–∏—Å—ã
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

-- –ü–æ–ª—É—á–∞–µ–º —Å—Å—ã–ª–∫—É –Ω–∞ –≤–∞—à–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
local player = Players.LocalPlayer
local character
local torso

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏ –æ–±—ä–µ–∫—Ç–∞
local function teleportObject(object, targetPosition)
    if object:IsA("BasePart") then
        object.CFrame = CFrame.new(targetPosition)
    elseif object:IsA("Model") then
        for _, child in ipairs(object:GetChildren()) do
            if child:IsA("BasePart") then
                child.CFrame = CFrame.new(targetPosition)
            end
        end
    end
end


local function findAndTeleport()
    if not player or not player.Character then return end
    character = player.Character or player.CharacterAdded:Wait()
    torso = character:FindFirstChild("Torso")

    if not torso then return end

    -- –ò—â–µ–º –æ–±—ä–µ–∫—Ç—ã M40, Deagle –∏ C4 –≤ Debris
    local debris = workspace.Debris
    local M40 = debris:FindFirstChild("M40")
    local Deagle = debris:FindFirstChild("Deagle")
    local PlantedC4 = debris:FindFirstChild("PlantedC4")
	local C4 = debris:FindFirstChild("C4")
	local Stim = debris:FindFirstChild("Stim") -- –î–æ–±–∞–≤–ª–µ–Ω–æ –æ–±–Ω–∞—Ä—É–∂–µ–Ω–∏–µ Stim

    -- –í—ã—á–∏—Å–ª—è–µ–º —Ü–µ–ª–µ–≤—É—é –ø–æ–∑–∏—Ü–∏—é (1 –º–µ—Ç—Ä –Ω–∏–∂–µ)
    local targetPosition = torso.Position - Vector3.new(0, 2.7, 0)

    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º M40
    if M40 then
        teleportObject(M40, targetPosition)
    else
        --warn("M40 –Ω–µ –Ω–∞–π–¥–µ–Ω!")
    end

    if Stim then
        teleportObject(Stim, targetPosition)
    else
        --warn("Stim –Ω–µ –Ω–∞–π–¥–µ–Ω!")
    end
    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º Deagle
    if Deagle then
        for _, child in ipairs(Deagle:GetChildren()) do
            teleportObject(child, targetPosition)
        end
    else
        --warn("Deagle –Ω–µ –Ω–∞–π–¥–µ–Ω!")
    end

    if C4 then
        teleportObject(C4, targetPosition) -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º C4
    else
        --warn("C4 –Ω–µ –Ω–∞–π–¥–µ–Ω!")
    end

    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º C4
    if PlantedC4 then
        teleportObject(PlantedC4, targetPosition) -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º PlantedC4
    else
        --warn("PlantedC4 –Ω–µ –Ω–∞–π–¥–µ–Ω!")
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é findAndTeleport()  –∫–∞–∂–¥—ã–µ 1 —Å–µ–∫—É–Ω–¥—É
RunService.Heartbeat:Connect(function(dt)
    -- –ó–∞–ø—É—Å–∫–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é, —Ç–æ–ª—å–∫–æ –µ—Å–ª–∏ –∏–≥—Ä–æ–∫ –∏ –ø–µ—Ä—Å–æ–Ω–∞–∂ —Å—É—â–µ—Å—Ç–≤—É—é—Ç
    if player and player.Character then
        findAndTeleport()
    end
end)

--üé©Send Notificationüé©
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

-- –°–æ–∑–¥–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –æ–ø–æ–≤–µ—â–µ–Ω–∏—è
local function sendNotification()
    local message = "github.com/NellyStart"
    
    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –≤—Å–µ–º –∏–≥—Ä–æ–∫–∞–º
    StarterGui:SetCore("SendNotification", {
        Title = "Auto Pickup",
        Text = message,
        Duration = 6 -- –ü—Ä–æ–¥–æ–ª–∂–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è –≤ —Å–µ–∫—É–Ω–¥–∞—Ö
    })
end

-- –í—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é
sendNotification()
